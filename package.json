{
  "name": "react-arc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "build": "npm run env:prod -- webpack",
    "coverage": "npm test -- --coverage",
    "dev": "npm run env:dev -- webpack-dev-server --watch-content-base --open",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "lint": "eslint src private webpack.config.js",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "prebuild": "npm run build:clean && npm run build:copy",
    "start": "npm run dev",
    "storybook": "start-storybook -p 9001 -c private/storybook",
    "test": "jest"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/fileMock.js",
      "^(components|containers)$": "<rootDir>/private/jest/componentsMock.js",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/private/jest/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/private/jest/actionsMock.js"
    },
    "setupFiles": [
      "<rootDir>/private/jest/setupTests.js"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.0.0",
    "delay": "^2.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "jest-cli": "^22.4.2",
    "opn-cli": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.5.1",
    "rimraf": "^2.6.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-blocks": "^1.0.0-rc.2",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "history": "^4.6.1",
    "lodash": "^4.17.5",
    "normalizr": "3.2.4",
    "prop-types": "^15.6.1",
    "query-string": "^5.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^4.0.0",
    "react-modal": "^3.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-form": "^7.3.0",
    "redux-saga": "^0.16.0",
    "redux-saga-thunk": "^0.6.0",
    "simple-load-script": "^1.0.2",
    "styled-components": "^3.2.1",
    "styled-theme": "^0.3.0",
    "styled-tools": "^0.4.0",
    "validator": "^9.4.1",
    "whatwg-fetch": "^2.0.2"
  }
}
